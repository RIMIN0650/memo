<?xml version="1.0" encoding="UTF-8"?>

  <!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.rimin.spring.memo.post.repository.PostRepository">

	<insert id="saveMemo" parameterType="map">
		INSERT INTO
			`post`
		(
			`title`
			, `comment`
		)
		VALUE
		(
			#{title}
			, #{comment}
		)
		
	
	</insert>


</mapper>